{
  "name": "pioneer-bot",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "npm": "npm i",
    "start": "node lib/index",
    "test": "npm run build && node lib/index",
    "build": "tsc -p .",
    "dev": "npm run build && npm run start",
    "prepublish": "npm run build",
    "docker:build": "docker build -t pioneer/pioneer-bot:latest .",
    "docker:tag": "docker tag pioneer/pioneer-bot:latest registry.digitalocean.com/pioneer/pioneer/pioneer-bot:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/pioneer/pioneer-bot:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push"
  },
  "dependencies": {
    "@pioneer-platform/accounting": "^6.2.2",
    "@pioneer-platform/ccbot-coincap": "^8.1.16",
    "@pioneer-platform/ccbot-easter-eggs": "^8.1.10",
    "@pioneer-platform/ccbot-rivescript-brain": "^8.1.10",
    "@pioneer-platform/coincap": "^8.1.5",
    "@pioneer-platform/default-mongo": "latest",
    "@pioneer-platform/default-redis": "latest",
    "@pioneer-platform/loggerdog": "latest",
    "@pioneer-platform/redis-queue": "^8.1.6",
    "asciichart": "^1.5.25",
    "chai": "^4.3.4",
    "dotenv": "^8.1.0",
    "node-cron": "^3.0.0",
    "openai": "^3.2.1",
    "sentence-tokenizer": "^1.0.1",
    "wait-promise": "^0.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^13.13.50",
    "nodemon": "^2.0.7",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  }
}
