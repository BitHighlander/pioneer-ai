{
  "name": "pioneer-discord-bridge",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "node": "node -v",
    "npm": "npm i",
    "start": "node lib/index",
    "test": "npm run build && node lib/index",
    "build": "tsc -p .",
    "dev": "npm run build && npm run start",
    "prepublish": "npm run build",
    "docker:build": "docker build -t pioneer/pioneer-discord-bridge:latest .",
    "docker:tag": "docker tag pioneer/discord-bridge:latest registry.digitalocean.com/pioneer/pioneer/pioneer-discord-bridge:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/pioneer/pioneer-discord-bridge:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push"
  },
  "dependencies": {
    "@pioneer-platform/accounting": "^6.2.2",
    "@pioneer-platform/coincap": "^8.1.5",
    "@pioneer-platform/default-mongo": "latest",
    "@pioneer-platform/default-redis": "latest",
    "@pioneer-platform/loggerdog": "latest",
    "@pioneer-platform/redis-queue": "latest",
    "@types/uuid": "^8.3.1",
    "chai": "^4.3.4",
    "discord.js": "^14.7.1",
    "dotenv": "^8.1.0",
    "node-cron": "^3.0.0",
    "sentence-tokenizer": "^1.0.1",
    "uuid": "^8.3.2",
    "wait-promise": "^0.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^13.13.50",
    "nodemon": "^2.0.7",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=6.0.0"
  }
}
